{
    "rules": {
        "preset-ja-technical-writing": true,
        "preset-ja-spacing": {
            "ja-space-between-half-and-full-width": {
              "space": "always"
            },
            "ja-space-around-code": {
                "before": true,
                "after": true
            }
        },
        "max-ten": {
            "max": 3
        },
        "spellcheck-tech-word": true,
        "no-start-duplicated-conjunction": true,
        "no-todo": true,
        "web-plus-db": true,
        "ja-no-abusage": true
    }
}
